### Sensors ###
# Sensors to Register separated by commas; options are clearnet,i2p,bote,tor,mesh
# A note on I2P: when registering bote, i2p will be included by default as bote is dependent on it.
1m5.sensors.registered=bote

# URL prefix and suffix (starts with period) routing rules for each sensor with multiple suffixes separated by commas
# These are not yet supported; hardcoded for now
#1m5.sensors.routing.bote=.bote
#1m5.sensors.routing.i2p=.i2p
#1m5.sensors.routing.tor=http,https,.onion
#1m5.sensors.routing.mesh=.mesh

## I2P Bote ##
# Default storageSpaceInbox is 1Gb (1073741824)
#1m5.sensors.i2p.bote.storageSpaceInbox=1073741824

# Default storageSpaceRelay is 100Mb (104857600)
#1m5.sensors.i2p.bote.storageSpaceRelay=104857600

# Default storageTime is 31 days in milliseconds (2678400000)
#1m5.sensors.i2p.bote.storageTime=2678400000

#1m5.sensors.i2p.bote.hashCashStrength=10
#1m5.sensors.i2p.bote.smtpPort=7661
#1m5.sensors.i2p.bote.smtpAddress=localhost
#1m5.sensors.i2p.bote.smtpEnabled=false
#1m5.sensors.i2p.bote.imapPort=7662
#1m5.sensors.i2p.bote.imapAddress=localhost
#1m5.sensors.i2p.bote.imapEnabled=false
# Dont uncomment the following password unless you know what you are doing; it will stomp over an auto-generated one
#1m5.sensors.i2p.bote.sslKeystorePassword=

# Default maxConcurIdCheckMail is 10 identities
#1m5.sensors.i2p.bote.maxConcurIdCheckMail=10

#1m5.sensors.i2p.bote.autoMailCheckEnabled=true
#1m5.sensors.i2p.bote.deliveryCheckEnabled=true

# Default check for mail interval if autoMailCheckEnabled is false in minutes is 30
#1m5.sensors.i2p.bote.mailCheckInterval=30

# Default outboxCheckInterval is 10 minutes
#1m5.sensors.i2p.bote.outboxCheckInterval=10

# Default deliveryCheckInterval is 60 minutes
#1m5.sensors.i2p.bote.deliveryCheckInterval=60

# Default relaySendPause is 10 minutes
#1m5.sensors.i2p.bote.RelaySendPause=10

# Controls whether strings that are added to outgoing email, like Re: or Fwd: are translated or note.
# Default for hideLocale is true
#1m5.sensors.i2p.bote.hideLocale=true

#1m5.sensors.i2p.bote.includeSentTime=true
#1m5.sensors.i2p.bote.messageIdCacheSize=1000

# The number of relay chains that should be used per Relay Request; default is 5
#1m5.sensors.i2p.bote.relayRedundancy=5

# The minimum amount of time in minutes that a Relay Request is delayed; default is 5
#1m5.sensors.i2p.bote.relayMinDelay=5

# The maximum amount of time in minutes that a Relay Request is delayed; default is 40
#1m5.sensors.i2p.bote.relayMaxDelay=40

# The number of relays that should be used when sending a DHT store request; default is 0
#1m5.sensors.i2p.bote.numSendHops=0

# gatewayDestination blank by default
#1m5.sensors.i2p.bote.gatewayDestination=

# gatewayEnabled is true by default allowing regular emails to be sent out to a configured gateway
1m5.sensors.i2p.bote.gatewayEnabled=false

# The number of minutes the password is kept in memory; default is 10
#1m5.sensors.i2p.bote.passwordCacheDuration=10

#1m5.sensors.i2p.bote.eeproxyHost=localhost
#1m5.sensors.i2p.bote.eeproxyPort=4444

